{
  "name": "laravel-web-tinker-helper",
  "version": "0.1.0",
  "description": "A little Chrome extension to help you manage all of your Laravel Tinker-web tabs.",
  "scripts": {
    "watch": "npm run development -- --watch",
    "dev": "npm run development",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --env=production --config=node_modules/laravel-mix/setup/webpack.config.js",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giuspe/laravel-tinker-web-helper-extension.git"
  },
  "keywords": [
    "chrome",
    "extension",
    "laravel",
    "tinker"
  ],
  "author": "giuspe <giuspe@gmail.com>",
  "bugs": {
    "url": "https://github.com/giuspe/laravel-tinker-web-helper-extension/issues"
  },
  "homepage": "https://github.com/giuspe/laravel-tinker-web-helper-extension#readme",
  "dependencies": {
    "cross-env": "^7.0.3",
    "laravel-mix": "^6.0.49",
    "postcss": "^8.4.38",
    "sass": "^1.77.2",
    "sass-loader": "^14.2.1",
    "tailwindcss": "^3.4.3",
    "vue": "^3.4.27",
    "vue-loader": "^17.4.2",
    "vue-template-compiler": "^2.7.16"
  }
}
